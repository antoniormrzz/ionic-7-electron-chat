{
  "name": "ionic-7-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "concurrently \"nodemon --exec npm run build\" \"npx cap open @capacitor-community/electron\"",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,tsx,js,jsx,json,css",
    "ignore": [
      "electron/*"
    ],
    "delay": "500"
  },
  "dependencies": {
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.4.1",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@maskito/core": "^1.7.0",
    "@maskito/react": "^1.7.0",
    "@pubnub/chat": "^0.1.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "ionicons": "^7.0.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@capacitor/cli": "5.4.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2",
    "wait-on": "^7.0.1"
  },
  "description": "An Ionic project"
}
